{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "version",
    "last_updated",
    "username",
    "name",
    "uid",
    "server",
    "creation"
  ],
  "properties": {
    "version": {
      "type": "integer"
    },
    "last_updated": {
      "type": "integer"
    },
    "username": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "likes": {
      "type": "integer"
    },
    "shares": {
      "type": "integer"
    },
    "reports": {
      "type": "integer"
    },
    "uid": {
      "type": "string"
    },
    "creation": {
      "type": "object",
      "required": [
        "wiki",
        "discord"
      ],
      "properties": {
        "wiki": {
          "type": "integer"
        },
        "discord": {
          "type": "integer"
        }
      }
    }
  }
}
